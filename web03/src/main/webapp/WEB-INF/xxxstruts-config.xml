<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE struts-config PUBLIC
	  "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
	  "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>
    <action-mappings>
	<!-- /html/addLeague  *.do actions -->
	<!-- Declare the Registration actions -->
	<action path="/html/addLeague"
		type="webServlet.control.AddLeagueAction">
	    <forward name="success" path="/html/succeedLeague.html"/>
	    <forward name="error" path="/html/addLeague.html"/>
	</action>
    </action-mappings>

</struts-config>
